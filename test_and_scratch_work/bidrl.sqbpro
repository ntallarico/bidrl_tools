where it.item_id = '14830715'</sql><sql name="SQL 15">drop TABLE if exists new_images;

-- Step 1: Create a new table with the desired structure
CREATE TABLE IF NOT EXISTS new_images (
    item_id TEXT,
    image_url TEXT,
    image_height INTEGER,
    image_width INTEGER,
    PRIMARY KEY (item_id, image_url)
);

-- Step 2: Copy data from the old table to the new table
INSERT INTO new_images (item_id, image_url, image_height, image_width)
SELECT item_id, image_url, image_height, image_width FROM images;

select * from images
select * from new_images


select item_id, image_url
, count(*) from images
group by item_id, image_url
order by count(*) desc

-- Step 3: Drop the old table
DROP TABLE images;

-- Step 4: Rename the new table to the original name
ALTER TABLE new_images RENAME TO images;</sql><current_tab id="2"/></tab_sql></sqlb_project>
