	(SELECT count(*) FROM auctions) as auctions
	, (SELECT count(*) FROM items) as items
	, (SELECT count(*) FROM bids) as bids
	, (SELECT count(*) FROM images) as images</sql><sql name="SQL 10">select im.image_url, it.description, a.title, it.url
