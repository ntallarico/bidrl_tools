                            <script type="text/javascript">
                                function toggleCheckbox(source) {
                                    checkboxes = $('.makeinvoice');
                                    for (var i = 0, n = checkboxes.length; i < n; i++) {
                                        checkboxes[i].checked = source.checked;
                                    }
                                }

                                var calculateIncrement = function(min_bid, input_bid, inc_scheme) {
                                    var inc_proxy = parseFloat(min_bid);
                                    while (inc_proxy < input_bid) {
                                        for (var index = 0; index < inc_scheme.length; index++) {
                                            if (inc_proxy >= parseFloat(inc_scheme[index].low) && inc_proxy <= parseFloat(inc_scheme[index].high)) {
                                                inc_proxy += parseFloat(inc_scheme[index].amount);
                                                inc_proxy = parseFloat(inc_proxy.toFixed(2));
                                                break;
                                            }
                                        }
                                    }
                                    return inc_proxy;
                                };

                                var showbidder = true;
                                var current_user = 168516;
                                var time_format = '';
                                var use_customer_number = false;
                                var holdsubmit = true;
                                var enable_blind_bidding = ((typeof (enable_blind_bidding) !== 'undefined') ? enable_blind_bidding : false);
                                var enable_bid_confirmation = false;
                                $(document).ready(function() {
                                    /*Send AJAX hit on form submit*/
                                    $('.indv_sub_invoice').click(function(e) {
                                        if ($("input[name='makeinvoice[]']:checked").length > 0) {
                                            $.post('/myaccount/additemstoInvoice/id/168516', $("#frmcbid").serialize(), function() {
                                                window.location.href = document.URL;
                                            });
                                        } else {
                                            smalert('', 'No item was selected. Please select some items first.');
                                            return false;
                                        }
                                        e.preventDefault();
                                    });

                                    $('#frmcbid').on('blur', '.bid-items input[type="text"]', function(ev, param) {
                                        var $this = $(this);
                                        var bid = parseFloat($this.val());
                                        if (!isCurrency(bid) || (bid == 0)) {
                                            return false;
                                        }
                                        var min_bid = $this.data('minimum_bid');
                                        var blind_bidding = $this.data('blind_bidding');
                                        min_bid = parseFloat(min_bid);
                                        if (force_increment_proxy && !blind_bidding) {
                                            var min_applicable_bid = min_bid;
                                            if ($this.data('current_user_winner')) {
                                                min_applicable_bid = $this.data('current_bid');
                                            }
                                            var increment = parseInt($this.data('increment'), 10);
                                            var flat_increment = parseFloat($this.data('flat_increment'));
                                            if (flat_increment > 0) {
                                                var net_proxy = parseFloat(bid - min_applicable_bid);
                                                var multiplier = Math.ceil((net_proxy / flat_increment).toFixed(2));
                                                inc_proxy = parseFloat((min_applicable_bid + (flat_increment * multiplier)).toFixed(2));
                                            } else {
                                                inc_proxy = calculateIncrement(min_applicable_bid, bid, inc_array[increment]);
                                            }
                                            if (inc_proxy != bid) {
                                                inc_msg = lang.auction_force_proxy_increment_msg.replace('%s', toCurrency(inc_proxy, decimals));
                                                smodal('', inc_msg, true, '<button class="modal-close">No</button><button class="ok">Yes</button>', function() {
                                                    $this.val(inc_proxy);
                                                    if (param && param == 'submit') {
                                                        setTimeout(function() {
                                                            $('.indv_sub').first().click();
                                                        }, 1000);
                                                    }
                                                }, true, function() {
                                                    $this.val('');
                                                }, 0, '');
                                            }
                                        }
                                    });

                                    //$('.bid-items div.inf').hover(showInfo, hideInfo);
                                    $('.indv_sub').click(function() {
                                        $('.bid-items input[type="text"][value!=""]:focus,.gallery input[type="text"][value!=""]:focus').trigger('blur', ['submit']);
                                        if ($('#chk-accept_terms_1,#chk-accept_terms_2').length > 0 && !$('#chk-accept_terms_1,#chk-accept_terms_2').is(':checked')) {
                                            smalert('', 'Please select the check box provided to indicate that you agree to the terms of this auction and then enter your bid again.');
                                            return false;
                                        }
                                        if (!holdsubmit)
                                            return true;
                                        var updateIds = [];
                                        $('.bid-items input[type="text"][value!=""],.gallery input[type="text"][value!=""]').each(function() {
                                            var inp = $(this);
                                            var inpval = $.trim(inp.val());
                                            if (inpval.length > 0) {
                                                updateIds.push(inp.attr('id').replace('iitem', ''));
                                                if (inp.parents('.bid-items').find('.aucterms :checkbox').length > 0) {
                                                    if (!inp.parents('.bid-items').find('.aucterms :checkbox').is(':checked')) {
                                                        smalert('', 'Please select the check box provided to indicate that you agree to the terms of this auction and then enter your bid again.');
                                                        return false;
                                                    }
                                                } else {
                                                    inp.parent().append('<input type="hidden" name="shipping_method" id="shipping_method" value="' + (shipping_method ? shipping_method : 'pickup') + '" />');
                                                }
                                            }
                                        });
                                        if (updateIds.length > 0) {
                                            $('.indv_sub').toggle();
                                            $('.dvprogress').toggle();

                                            $.post('/itemdetail/itemresults', {
                                                itemids: updateIds.join(','),
                                                'time_format': time_format
                                            }, function(data) {
                                                if (typeof (data) != 'object')
                                                    location.href = location.href;
                                                if (typeof (handleItemResults) != 'undefined') {
                                                    handleItemResults(data);
                                                    return;
                                                }
                                                displayBidConfirmation();
                                                $('.indv_sub').toggle();
                                                $('.dvprogress').toggle();
                                            });
                                        } else {
                                            smalert('Bid Summary', '<div class="red">You did not enter any bids.</div>', true);
                                            return false;
                                        }
                                        return false;
                                    });
                                });

                                var displayBidConfirmation = function() {
                                    var modtext = '';
                                    var total = 0;
                                    $('.bid-items input[type="text"],.gallery input[type="text"]').each(function(index, el) {
                                        el = $(el);
                                        message = '';
                                        if ($.trim(el.val()).length > 0) {
                                            /*HN 10/18/2011 - get attributes and do JS validation*/
                                            current_bid = parseFloat(el.data('current_bid'));
                                            proxy_bid = parseFloat(el.data('proxy_bid'));
                                            minimum_bid = parseFloat(el.data('minimum_bid'));
                                            premium = parseFloat(el.data('premium'));
                                            has_reserve = el.data('has_reserve');
                                            reserve_met = el.data('reserve_met');
                                            current_user_winner = el.data('current_user_winner');
                                            quantity = parseInt(el.data('quantity'));
                                            buy_now = parseFloat(el.data('buy_now'));
                                            blind_bidding = parseFloat(el.data('blind_bidding'));
                                            new_value = el.val();
                                            if ((!isCurrency(new_value)) || (new_value == 0)) {
                                                message += 'Your bid is not a valid number. Please enter valid numbers like the following:<br /> 95 &nbsp;&nbsp;&nbsp; $95.5 &nbsp; $95,555.55';
                                                message = message.format(toCurrency(minimum_bid, decimals));
                                                row_class = 'red';
                                            } else if (enable_blind_bidding && blind_bidding) {
                                                new_value = currencyToNum(new_value, decimals);
                                                multiitemval = toCurrency(new_value, decimals);
                                                total += new_value * (quantity > 1 ? quantity : 1) + (new_value * (quantity > 1 ? quantity : 1) * parseFloat(premium) / 100);
                                                if (quantity > 1) {
                                                    multiitemval = toCurrency(new_value, decimals) + ' x ' + quantity + ' = ' + toCurrency(parseFloat(new_value * quantity), decimals);
                                                }
                                                message += 'You are about to bid {0}. ';
                                                message = message.format(multiitemval);
                                                row_class = 'green';
                                            } else if (buy_now > 0 && new_value >= buy_now) {
                                                buy_now = buy_now * (quantity > 1 ? quantity : 1);
                                                message += 'You have entered bid amount which is greater or equal to buy now price of the item. You are about to purchase this item for [buy_now_price]. After the auction is over this item will be added to your invoice from the auction.';
                                                message = message.replace('[buy_now_price]', toCurrency(buy_now, decimals))
                                                row_class = 'green';
                                                total += buy_now + (buy_now * premium / 100);
                                            } else {
                                                new_value = currencyToNum(new_value, decimals);
                                                multiitemval = toCurrency(new_value, decimals);
                                                total += new_value * (quantity > 1 ? quantity : 1) + (new_value * (quantity > 1 ? quantity : 1) * premium / 100);
                                                if (quantity > 1) {
                                                    multiitemval = toCurrency(new_value, decimals) + ' x ' + quantity + ' = ' + toCurrency(parseFloat(new_value * quantity), decimals);
                                                }
                                                message = 'You are about to bid {0}. ';
                                                message = message.format(multiitemval);
                                                if (parseInt(current_user_winner) && (!has_reserve || reserve_met)) {
                                                    //bidder already current high bidder
                                                    if (!(proxy_bid > 0)) {
                                                        //no proxy bid
                                                        if (new_value > current_bid) {
                                                            //new bid greater than current bid
                                                            message += 'You will have a proxy bid of {0} on this item.';
                                                            message = message.format(toCurrency(new_value, decimals));
                                                            row_class = 'green';
                                                        } else if (new_value < current_bid) {
                                                            //new bid less than current bid
                                                            message += 'You will not be able to bid lower than the current bid.';
                                                            row_class = 'red';
                                                        } else if (new_value == current_bid) {
                                                            //new bid equal to current bid
                                                            message += 'The bid you are placing is the same as your current bid so nothing will be changed.';
                                                            message = message.format(multiitemval);
                                                            row_class = 'green';
                                                        }
                                                    } else {
                                                        //its a proxy bid
                                                        if (new_value > proxy_bid) {
                                                            //new bid greater than current proxy bid
                                                            message += 'Your proxy bid will be increased.<br/> You will have a proxy bid of {0} on this item.';
                                                            message = message.format(toCurrency(new_value, decimals));
                                                            row_class = 'green';
                                                        } else if (new_value < proxy_bid) {
                                                            //new bid less than current proxy big
                                                            if (new_value == current_bid) {
                                                                //new bid equal to current bid
                                                                message += 'We will delete your proxy bid.';
                                                                row_class = 'green';
                                                            } else if (new_value > current_bid) {
                                                                //new bid greater than current bid
                                                                message += 'We will lower your proxy bid.';
                                                                row_class = 'green';
                                                            } else if (new_value < current_bid) {
                                                                //new bid is less than current bid
                                                                message += 'You will not be able to bid lower than the current bid.';
                                                                row_class = 'red';
                                                            }
                                                        } else if (new_value == proxy_bid) {
                                                            //new bid is equal to proxy bid
                                                            message += 'The amount that you bid, {0}, is already your proxy bid.<br/> So nothing will change.';
                                                            message = message.format(multiitemval);
                                                            row_class = 'red';
                                                        }
                                                    }
                                                } else {
                                                    //bidder is not the current high bidder
                                                    if (new_value >= minimum_bid) {
                                                        row_class = 'green';
                                                    } else {
                                                        message += 'Your bid is less than the minimum bid for this item ({0}).';
                                                        message = message.format(toCurrency(minimum_bid, decimals));
                                                        row_class = 'red';
                                                    }
                                                }
                                            }
                                            /*HN 10/18/2011 - get attributes and do JS validation*/
                                        }
                                        if (message != '') {
                                            modtext += buildItemRow(row_class, el.data('title'), message);
                                        }
                                    });
                                    modtext = '<b>Here&#39;s a summary of items you are about to bid on:</b><br/>' + modtext + '<br/>';
                                    smodal('Bid Summary', modtext, true, '<button class="modal-close">Cancel</button><button class="ok">OK</button>', function() {
                                        holdsubmit = false;
                                        $('.indv_sub').eq(0).click();
                                    }, false);

                                };
                                var buildItemRow = function(rowclass, heading, message) {
                                    return '<div><b class="' + rowclass + '">' + heading + '</b><br/>' + message + '</div>';
                                };
                            </script>
