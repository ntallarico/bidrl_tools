                                        "pre_deposit_amount": "0.00",
                                        "mapping_address": "161 Johns Rd. Unit A",
                                        "mapping_city": "Greer",
                                        "mapping_state": "123",
                                        "mapping_zip": "29650",
                                        "mapping_county": "",
                                        "execution_time": null,
                                        "description": "",
                                        "state": "SC ",
                                        "highbidder_username": "IsleyJ",
                                        "custombidder_username": null,
                                        "category_id": null,
                                        "image_url": "auctionimages\/152706\/cp1713565538x19786424\/wSB2101IMG_0039.JPG",
                                        "thumb_url": "https:\/\/d3ugkdpeq35ojy.cloudfront.net\/auctionimages\/152706\/cp1713565538x19786424\/wSB2101IMG_0039_t.JPG",
                                        "on_cdn": "1",
                                        "current_user_winner": false,
                                        "winner": "IsleyJ",
                                        "box_footer": [],
                                        "show_bid_history": true,
                                        "auction_url": "kitchen-goods-auction-161-johns-rd-unit-a-south-carolina-april-25-152706",
                                        "current_time": 1713834827,
                                        "time_offset": 3600,
                                        "has_reserve": false,
                                        "reserve_met": false,
                                        "effective_increment": "0.25"
                                    }];
                                    return data;
                                });
                                myAccount.controller('myItems', ['$scope', 'ItemService', '$filter', '$timeout', '$interval', 'ngProgress', 'myAccountData', function($scope, ItemService, $filter, $timeout, $interval, ngProgress, myAccountData) {
                                    // Initialization
                                    $scope.items = myAccountData.items;
                                    $scope.current_user = current_user;
                                    $scope.has_bids = has_bids_in_item;
                                    $scope.show_hide_closed = show_hide_closed;
                                    $scope.lang = lang;
                                    $scope.current_sort = sort;
                                    $scope.auction_terms = auction_terms;
                                    $scope.crowd_bidder = crowd_bidder;
                                    $scope.crowd_bidder_username = crowd_bidder_username;
                                    $scope.live_online_live_id = '17';
                                    $scope.disable_bid_history = typeof (disable_bidhistory_after_sale) !== 'undefined' ? disable_bidhistory_after_sale : 0;
                                    $scope.show_bid_history = true;
                                    $scope.pausedchannel = [];
                                    $scope.enable_blind_bidding = enable_blind_bidding;
                                    $scope.lot_number_hide_leading_zeroes = lot_number_hide_leading_zeroes;
                                    var itemGroupIds = [];

                                    $scope.timesync = function(refresh) {
                                        refresh = refresh || false;
                                        $scope.current_time = Date.now() / 1000;
                                        $scope.time_difference = 0;
                                        $scope.latency_calculated = false;
                                        if (refresh && angular.isDefined($scope.current_interval)) {
                                            $interval.cancel($scope.current_interval);
                                            $scope.current_interval = undefined;
                                        }
                                        $.ajax({
                                            type: 'GET',
                                            dataType: 'json',
                                            url: '/timesync.json',
                                            async: false,
                                            success: function(stime) {
                                                var tend = Date.now() / 1000;
                                                $scope.time_difference = ((stime - $scope.current_time) - (tend - $scope.current_time));
                                                if (realtime && (!angular.isDefined($scope.current_interval) || refresh)) {
                                                    // Interval is updating current time of the scope
                                                    $scope.current_interval = $interval(function() {
                                                        if (!$scope.latency_calculated) {
                                                            $scope.current_time = Math.round((Date.now() / 1000) + $scope.time_difference);
                                                            $scope.latency_calculated = true;
                                                        } else {
                                                            if (Math.abs(Math.round((Date.now() / 1000) + $scope.time_difference) - $scope.current_time) > 5) {
                                                                $scope.timesync(true);
                                                            } else {
                                                                $scope.current_time = Math.round((Date.now() / 1000) + $scope.time_difference);
                                                            }
                                                        }
                                                    }, 1000);
                                                }
                                            }
                                        });
                                    }
                                    ;
                                    $scope.timesync();

                                    // Init function of the scope items collection
                                    $scope.initData = function() {
                                        // If auction is ongoing and realtime is enabled then initialize pusher API
                                        if (realtime && !angular.isDefined($scope.pusher)) {
                                            $scope.pusher = new Pusher(pusher_api_key,{
                                                cluster: pusher_cluster,
                                                disableStats: true,
                                                pong_timeout: 20000,
                                                activity_timeout: 10000
                                            });

                                            //   Enable for logging pusher events on development, not for production environment
                                            //      Pusher.log = function(message) {
                                            //        if (window.console && window.console.log) {
                                            //          window.console.log(message);
                                            //        }
                                            //      };

                                            var first_pusher_connection = true;
                                            $scope.pusher.connection.bind('state_change', function(states) {
                                                if (states.previous === 'initialized' && states.current === 'connecting') {
                                                    return;
                                                }
                                                if (states.current !== 'connected') {
                                                    $timeout(function() {
                                                        $scope.show_pusher_warning_alert = true;
                                                    }, 3000);
                                                }
                                                if ((!first_pusher_connection) && states.current === 'connected') {
                                                    $scope.show_pusher_warning_alert = false;
                                                    window.location.reload();
                                                }
                                                if (states.current === 'connected') {
                                                    first_pusher_connection = false;
                                                }
                                            });

                                            //completed auction pusher event
                                            $scope.auction_completed_channel = $scope.pusher.subscribe(domain_name + '-myaccount_completed_auction');
                                            $scope.auction_completed_channel.bind('myaccount_completed_auction', function(data) {
                                                $scope.$apply(function() {
                                                    for (indx = 0; indx < $scope.items.length; indx++) {
                                                        if ($scope.items[indx].auction_groupid == data.auction_id) {
                                                            $scope.items[indx].completed = data.completed;
                                                        }
                                                    }
                                                });
                                            });

                                        }
                                        for (indx = 0; indx < $scope.items.length; indx++) {
                                            $scope.update_item($scope.items[indx]);
                                            if (itemGroupIds.indexOf($scope.items[indx].groupid) < 0 && $scope.items[indx].groupid != 0) {
                                                itemGroupIds.push($scope.items[indx].groupid);
                                            }
                                        }
                                        $scope.softCloseGroupPusherUpdates();
                                        if (typeof (pusher_api_key) !== 'undefined' && realtime && enable_ajax_pusher_updates) {
                                            $scope.watchItemsClosing();
                                        }
                                    }
                                    ;
                                    // soft close group trigers
                                    $scope.softCloseGroupPusherUpdates = function() {
                                        $scope.softCloseChannel = [];
                                        for (var indx1 = 0; indx1 < itemGroupIds.length; indx1++) {
                                            if (realtime && !angular.isDefined($scope.softCloseChannel[itemGroupIds[indx1]])) {
                                                // Subscribe to soft close pusher API  channel   
                                                $scope.softCloseChannel[itemGroupIds[indx1]] = $scope.pusher.subscribe(domain_name + '-softclosegroup-' + itemGroupIds[indx1]);
                                            }
                                            if (realtime && angular.isDefined($scope.softCloseChannel[itemGroupIds[indx1]])) {
                                                $scope.softCloseChannel[itemGroupIds[indx1]].bind('softclose', function(data) {
                                                    $scope.$apply(function() {
                                                        var item = jQuery.extend(true, item, data.items);
                                                        for (var indx2 = 0; indx2 < data.items.length; indx2++) {
                                                            for (var indx = 0; indx < $scope.items.length; indx++) {
                                                                if ($scope.items[indx].groupid === item[indx2].groupid) {
                                                                    $scope.items[indx].end_time = item[indx2].end_time;
                                                                    $scope.items[indx].bidding_extended = item[indx2].bidding_extended;
                                                                    $scope.update_item($scope.items[indx], true);
                                                                }
                                                            }
                                                        }

                                                    });
                                                });
                                            }
                                        }
                                    }
                                    ;

                                    // Destroy scope members and API connections
                                    $scope.destroy = function() {
                                        for (indx = 0; indx < $scope.items.length; indx++) {
                                            if (angular.isDefined($scope.items[indx].channel)) {
                                                $scope.pusher.unsubscribe(domain_name + '-item-' + $scope.items[indx].id);
                                                $scope.items[indx].channel = undefined;
                                            }
                                            if (angular.isDefined($scope.pausedchannel[$scope.items[indx].auction_groupid])) {
                                                $scope.pusher.unsubscribe(domain_name + '-auction-' + $scope.items[indx].auction_groupid);
                                            }
                                        }
                                        if (angular.isDefined($scope.watch_interval)) {
                                            $interval.cancel($scope.watch_interval);
                                        }
                                        $scope.watch_interval = undefined;
                                    }
                                    ;
                                    // Scope event triggered by angular JS
                                    $scope.$on('$destroy', function() {
                                        $scope.destroy();
                                    });

                                    // Item initialization and update function
                                    $scope.update_item = function(item, ignorePusher) {
                                        if (typeof (ignorePusher) === 'undefined') {
                                            ignorePusher = false;
                                        }
                                        item.end_time = parseInt(item.end_time, 10);
                                        item.start_time = parseInt(item.start_time, 10);
                                        item.quantity = parseInt(item.quantity, 10);
                                        item.high_bidder = parseInt(item.high_bidder, 10);
                                        item.buy_now = parseFloat(item.buy_now);
                                        item.current_bid = parseFloat(item.current_bid);
                                        item.minimum_bid = parseFloat(item.minimum_bid);
                                        item.time_of_bid = parseInt(item.time_of_bid, 10);
                                        item.execution_time = parseInt(item.execution_time, 10);
                                        item.pre_deposit_amount = parseInt(item.pre_deposit_amount, 10);
                                        item.free_deposit_amount = parseInt(item.free_deposit_amount, 10);

                                        item.blind_bidding = (item.blind_bidding && item.blind_bidding != '0');
                                        if ($scope.disable_bid_history && (angular.isDefined(item.hide_bidhistory_after_sale) && parseInt(item.hide_bidhistory_after_sale)) && item.start_time > 0 && item.start_time <= ($scope.current_time + item.time_offset) && item.end_time <= ($scope.current_time + item.time_offset)) {

                                            item.show_bid_history = false;
                                        }

                                        // Item box highlighting code
                                        if (!angular.isDefined(item.highlight)) {
                                            item.highlight = '';
                                        }
                                        item.url = $filter('slugify')(item.title, item.auction_url, item.id);

                                        item.time_not_set = false;
                                        if (parseInt(item.end_time) <= 0 || parseInt(item.start_time) <= 0) {
                                            item.time_not_set = true;
                                        }

                                        item.status = '';
                                        item.status_text = '';
                                        if ($scope.has_bids[item.id] > 0 && item.current_user_winner) {
                                            item.status = 'winning';
                                            item.status_text = lang.winning_status;
                                        } else if ($scope.has_bids[item.id] > 0) {
                                            item.status = 'losing';
                                            item.status_text = lang.losing_status;
                                        }
                                        if (item.has_reserve) {
                                            item.reserve_price_display = ((item.reserve_option != 'none') ? ((!item.reserve_met) ? ((item.reserve_option == 'standard') ? 'There is a reserve price that has <b>NOT</b> been met.' : 'Pending Approval') : ((item.reserve_option == 'standard') ? 'The reserve price <b>HAS</b> been met.' : 'Sells to High Bidder')) : '');
                                        }
                                        item.extended_tip = lang.auction_extended_bidding_tip.replace('%s', item.extended_bidding_threshold).replace('%s', (item.extended_bidding_interval < 0 ? item.extended_bidding_threshold : item.extended_bidding_interval));
                                        // Subscribe item to pusher API item channel
                                        if (realtime && item.start_time <= ($scope.current_time + item.time_offset) && item.end_time > ($scope.current_time + item.time_offset) && !ignorePusher && !angular.isDefined(item.channel)) {
                                            item.channel = $scope.pusher.subscribe(domain_name + '-item-' + item.id);
                                        }

                                        if (!angular.isDefined($scope.pausedchannel[item.auction_groupid])) {
                                            $scope.pausedchannel[item.auction_groupid] = $scope.pusher.subscribe(domain_name + '-auction-' + item.auction_groupid);
                                        }
                                        if (angular.isDefined($scope.pausedchannel[item.auction_groupid])) {
                                            $scope.pausedchannel[item.auction_groupid].bind('pause', function(data) {
                                                window.location.reload();
                                                return;
                                            });
                                            $scope.pausedchannel[item.auction_groupid].bind('bid_increment', function(data) {
                                                window.location.reload();
                                                return;
                                            });
                                        }

                                        // Bind item to pusher API item channel callback function
                                        if (realtime && item.start_time <= ($scope.current_time + item.time_offset) && (item.end_time - ($scope.current_time + item.time_offset)) > 0 && !ignorePusher && angular.isDefined(item.channel)) {
                                            item.channel.bind('minimum_bid', function(data) {
                                                $scope.$apply(function() {
                                                    item.minimum_bid = data.minimum_bid;
                                                    item.flat_increment = data.flat_increment;
                                                    item.increment = data.increment;
                                                    item.effective_increment = data.effective_increment;
                                                });
                                            });
                                            item.channel.bind('bid', function(data) {
                                                $scope.$apply(function() {
                                                    data.item.current_user_winner = parseInt(data.item.high_bidder, 10) === parseInt(current_user, 10);
                                                    data.item.winner = data.item.highbidder_username;
                                                    if (data.item.current_user_winner) {
                                                        item.highlight = 'highlightg';
                                                        $timeout(function() {
                                                            item.highlight = '';
                                                        }, 3000);
                                                    } else {
                                                        item.highlight = 'highlightr';
                                                        $timeout(function() {
                                                            item.highlight = '';
                                                        }, 3000);
                                                    }
                                                    item = jQuery.extend(true, item, data.item);
                                                    if (!data.item.current_user_winner) {
                                                        item.proxy_bid = 0;
                                                    }
                                                    item.result = undefined;
                                                    $scope.update_item(item, true);
                                                });
                                            });
                                            item.channel.bind('pusher:subscription_succeeded', function(data) {
                                                if (parseInt(this.item.high_bidder, 10) === parseInt(current_user, 10)) {
                                                    $scope.getPusherAJAX(this.item);
                                                }
                                            }, {
                                                item: item
                                            });
                                        }
                                    }
                                    ;

                                    $scope.watchItemsClosing = function() {
                                        if (!angular.isDefined($scope.watch_interval)) {
                                            $scope.watch_interval = $interval(function() {
                                                for (var index = 0; index < $scope.items.length; index++) {
                                                    (function(i) {
                                                        if (!angular.isDefined($scope.items[i].closed)) {
                                                            $scope.items[i].rtime = $scope.items[i].end_time - ($scope.current_time + $scope.items[i].time_offset);
                                                            if ($scope.items[i].rtime <= 10) {
                                                                $scope.getPusherAJAX($scope.items[i]);
                                                            }
                                                        }
                                                    }
                                                    )(index);
                                                }
                                            }, 5000);
                                        }
                                    }
                                    ;

                                    $scope.getPusherAJAX = function(item) {
                                        var path = '/aucbeat/pusher/' + item.auction_groupid + '-' + item.id + '.json';
                                        $.get(path, function(data) {
                                            if (angular.isDefined(data.item)) {
                                                data.item.current_bid = parseFloat(data.item.current_bid);
                                                item.current_bid = parseFloat(item.current_bid);
                                                if (data.item.current_bid > item.current_bid) {
                                                    data.item.current_user_winner = parseInt(data.item.high_bidder, 10) === parseInt(current_user, 10);
                                                    data.item.winner = data.item.highbidder_username;
                                                    if (data.item.current_user_winner) {
                                                        item.highlight = 'highlightg';
                                                        $timeout(function() {
                                                            item.highlight = '';
                                                        }, 3000);
                                                    } else {
                                                        item.highlight = 'highlightr';
                                                        $timeout(function() {
                                                            item.highlight = '';
                                                        }, 3000);
                                                    }
                                                    item = jQuery.extend(true, item, data.item);
                                                    item.result = undefined;
                                                    if (item.groupid > 0 && item.bidding_extended) {
                                                        for (var indx = 0; indx < $scope.items.length; indx++) {
                                                            if ($scope.items[indx].groupid === item.groupid) {
                                                                $scope.items[indx].end_time = item.end_time;
                                                                $scope.items[indx].bidding_extended = item.bidding_extended;
                                                                $scope.update_item(item, true);
                                                            }
                                                        }
                                                    }
                                                    $scope.update_item(item, true);
                                                } else if (item.rtime < -5) {
                                                    item.closed = true;
                                                    if ($scope.disable_bid_history && (angular.isDefined(item.hide_bidhistory_after_sale) && parseInt(item.hide_bidhistory_after_sale))) {
                                                        item.show_bid_history = false;
                                                    }
                                                }
                                            } else if (item.rtime < -5) {
                                                item.closed = true;
                                                if ($scope.disable_bid_history && (angular.isDefined(item.hide_bidhistory_after_sale) && parseInt(item.hide_bidhistory_after_sale))) {
                                                    item.show_bid_history = false;
                                                }
                                            }
                                        });
                                    }
                                    ;

                                    $scope.clear_message = function(index) {
                                        delete $scope.items[index].bid_result;
                                    }
                                    ;

                                    var destroyPusher = function() {
                                        $scope.destroy();
                                        if ($scope.pusher) {
                                            $scope.pusher.disconnect();
                                            $scope.show_pusher_warning_alert = false;
                                        }
                                    };

                                    if (window.addEventListener) {
                                        window.addEventListener("beforeunload", destroyPusher);
                                    } else {
                                        window.attachEvent("beforeunload", destroyPusher);
                                    }
                                    $scope.initData();

                                    // Process remote fetched items data
                                    $scope.applyRemoteData = function(data) {
                                        $scope.destroy();
                                        if (data.items.length < 1) {
                                            location.reload();
                                        }
                                        $scope.items = data.items;
                                        $scope.initData();
                                        setTimeout(initTips, 1);
                                    }
                                    ;

                                    // Fetch remote items data
                                    $scope.loadRemoteData = function() {
                                        ngProgress.start();
                                        ItemService.getItems($scope.auction_id, $scope.current_user, $scope.pager).then(function(data) {
                                            $scope.applyRemoteData(data);
                                        });
                                        ngProgress.complete();
                                    }
                                    ;

                                    $scope.refreshPage = function() {
                                        $scope.loadRemoteData();
                                    }
                                    ;
                                    $scope.changeSort = function(current_sort) {
                                        $scope.current_sort = current_sort;
                                        $scope.loadRemoteData();
                                    }
                                    ;
                                    $scope.$on('itemRenderFinished', function(ngRepeatFinishedEvent) {
                                        $('#angularapp').show();
                                        initTips();
                                        if ($('.msg').length > 0) {
                                            ng_bidresponsemsg();
                                        }
                                        if ($('table.grid .item-img').length > 0) {
                                            $('.grid td img').jail();
                                        }
                                    });
                                    clearTimeout(jsfmsgtimer);
                                    $('#jsfmsg').remove();
                                    $timeout(function() {
                                        $scope.timesync(true);
                                    }, 2000);
                                }
                                ]);
                            </script>
                            <fieldset class="pager">
                                <div>Page 1 of 6 &nbsp;&nbsp;&nbsp;&nbsp;</div>
                                <div>
                                    <form action="/myaccount/myitems">
                                        <label for="pager-bottom">Go to page</label>
                                        <select id="pager-bottom" name="page">
                                            <option value="1" selected="selected">1</option>
                                            <option value="2">2</option>
