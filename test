                    <script type="text/javascript">
                        var closeModal = function() {
                            $.modal.close();
                            $('#ifmodal').remove();
                        };
                        function showPaymentTerms() {
                            var htm = '<div>I hereby agree that I will pay invoice for items I win in any auction within 5 days of auction closing date. Delaying payments and/or non payment of invoice may result in permanent ban on the account.</div>';
                            htm += '<div style="margin-top: 20px;"><input type="button" id="btn_ok" value="I Agree" />' + '<input type="button" id="btn_cancel" value="Cancel" /></div>';
                            $('#wrapper').after('<div class="sbblock" id="ifmodal"><h3><span>Payment Terms</span></h3>' + htm + '</div>');
                            $('#ifmodal').modal({
                                closeHTML: "<a title='Close' class='modal-close'>X</a>",
                                position: [200],
                                maxWidth: 800,
                                overlayClose: false,
                                fixed: false,
                                overlayId: 'modal-overlay',
                                containerId: 'modal-container',
                                closeClass: 'modal-close',
                                onClose: closeModal
                            });
                            $('#btn_cancel').click(closeModal);
                            $('#btn_ok').click(function() {
                                $.post('/myaccount/paymenttermsagreed', function(response) {
                                    if (response.status === 'success') {
                                        location.reload();
                                    }
                                    closeModal();
                                });
                            });
                        }
                        ;function showPickupTerms() {
                            var htm = '<div>All items won in any auction must be picked up on time. Buyers will  blacklisted along with no refunds or exchanges. Please bid accordingly.</div>';
                            htm += '<div style="margin-top: 20px;"><input type="button" id="btn_ok" value="I Agree" />' + '<input type="button" id="btn_cancel" value="Cancel" /></div>';
                            $('#wrapper').after('<div class="sbblock" id="ifmodal"><h3><span>Pickup Terms</span></h3>' + htm + '</div>');
                            $('#ifmodal').modal({
                                closeHTML: "<a title='Close' class='modal-close'>X</a>",
                                position: [200],
                                maxWidth: 600,
                                overlayClose: false,
                                fixed: false,
                                overlayId: 'modal-overlay',
                                containerId: 'modal-container',
                                closeClass: 'modal-close',
                                onClose: function() {
                                    $.modal.close();
                                    $('#ifmodal').remove();
                                }
                            });
                            $('#btn_cancel').click(function() {
                                $.modal.close();
                                $('#ifmodal').remove();
                            });
                            $('#btn_ok').click(function() {
                                $.post('/myaccount/pickuptermsagreed', function(response) {
                                    if (response.status === 'success') {
                                        location.reload();
                                    }
                                });
                                $.modal.close();
                                $('#ifmodal').remove();
                            });
                        }
