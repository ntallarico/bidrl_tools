                                var jsfmsgtimer = setTimeout(function() {
                                    $('#jsfmsg').show();
                                }, 1000);
                                var myAccount = angular.module('myAccount', ['ngProgress', 'commonFunctions']);
                                myAccount.factory('myAccountData', function() {
