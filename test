                            <br/>
                            <button id="btnClearAll">Clear All</button>
                            <script type="text/javascript">
                                $(document).ready(function() {
                                    $('#btnClearAll').click(function(ev) {
                                        ev.preventDefault();
                                        var auc_id = '';
                                        var confirmation_msg = "";
                                        if (!auc_id) {
                                            auc_id = 'all';
                                            confirmation_msg = "Are you sure you want to clear all your items' list?";
                                        } else {
                                            var auc_title = $("#sauctions>option:selected").text();
                                            confirmation_msg = "Are you sure you want to clear the items of {title} from your items' list?".replace('{title}', auc_title);
                                        }
                                        smconfirm('', (confirmation_msg), function() {
                                            location.href = ('/myaccount/removeallmyitems/auction/' + auc_id);
                                        });
                                        return false;
                                    });
                                });
                            </script>
