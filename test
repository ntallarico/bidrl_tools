                                    $('#perpage-top,#perpage-bottom').change(function() {
                                        $(this).parents('form').submit();
                                    });

                                    $('#pager-top,#pager-bottom').change(function() {
                                        window.location.href = '/myaccount/myitems' + '/page/' + $(this).val();
                                    });
                                });
