            $(document).ready(function() {
                $('.impinfo').click(function(ev) {
                    ev.preventDefault();
                    var src = $(this);
                    src.after('<div class="sbblock iframe" id="ifmodal"><h3><span>Important Information</span></h3><iframe style="border:none;height:250px;width:101.2%;margin:-8px -6px -16px;"></iframe></div>');
                    $('#ifmodal').modal({
                        closeHTML: "<a title='Close' class='modal-close'>X</a>",
                        position: [30],
                        minWidth: 1024,
                        maxWidth: 1024,
                        overlayClose: true,
                        fixed: false,
                        overlayId: 'modal-overlay',
                        containerId: 'modal-container',
                        closeClass: 'modal-close',
                        onClose: function(refresh) {
                            $.modal.close();
                            $('#ifmodal').remove();
                            if (typeof (refresh) !== 'undefined' && refresh === true) {
                                window.location.reload();
                            }
                        },
                        onShow: function() {
                            $('#modal-container').find('iframe').attr('src', '/importantinfo/index/' + src.data('id'));
                        }
                    });
                });
            });
        </script>
